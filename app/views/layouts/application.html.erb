<!DOCTYPE html>
<html>
  <head>
    <title>Art&Decorações</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
      <!-- load CSS -->
    <%= stylesheet_link_tag "https://fonts.googleapis.com/css?family=Open+Sans:300,400,600"%>
    <%= stylesheet_link_tag "vendor/bootstrap/css/bootstrap.min.css" %>
    <%= stylesheet_link_tag "vendor/fontawesome-free/css/all.min.css" %>
    <%= stylesheet_link_tag "https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800" %>
    <%= stylesheet_link_tag "https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic" %>
    <%= stylesheet_link_tag "vendor/magnific-popup/magnific-popup.css" %>
    <%= stylesheet_link_tag "css/creative.min.css" %>
  </head>

  <body>
    <%= yield %>
    <%= javascript_include_tag "vendor/jquery/jquery.min.js" %>
    <%= javascript_include_tag "vendor/bootstrap/js/bootstrap.bundle.min.js" %>
    <%= javascript_include_tag "vendor/jquery-easing/jquery.easing.min.js" %>
    <%= javascript_include_tag "vendor/scrollreveal/scrollreveal.min.js" %>
    <%= javascript_include_tag "vendor/magnific-popup/jquery.magnific-popup.min.js" %>
    <%= javascript_include_tag "js/creative.min.js" %>
  </body>
</html>
